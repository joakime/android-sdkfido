<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>@GROUPID@</groupId>
    <artifactId>@PARENT_ARTIFACTID@</artifactId>
    <version>@VERSION@</version>
  </parent>
  <groupId>@GROUPID@</groupId>
  <artifactId>@ARTIFACTID@</artifactId>
  <packaging>jar</packaging>
  <build>
    <plugins>
      <plugin>
        <groupId>net.erdfelt.android.sdkfido</groupId>
        <artifactId>sdkfido-maven-plugin</artifactId>
        <version>1.0-SNAPSHOT</version>
        <executions>
          <execution>
            <id>build-aidl</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>aidl</goal>
            </goals>
            <configuration>
              <excludes>
                <exclude>**/com/android/internal/view/IInputConnectionCallback.aidl</exclude>
              </excludes>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies />
</project>
